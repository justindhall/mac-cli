{
  "name": "@idme/mac-cli",
  "version": "1.0.0",
  "description": "A place for mac cli aliases and convenience methods",
  "main": "./dist/js/index.js",
  "bin": {
    "github": "./dist/js/index.js"
  },
  "scripts": {
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "create": "npm run build && npm run test",
    "build": "tsc -p .",
    "watch": "npm run build && npm run start",
    "test": "sudo npm i -g && github",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/idme/mac-cli.git"
  },
  "keywords": [
    "mac",
    "cli"
  ],
  "author": "samuelbirk",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/samuelbirk-ops/mac-cli/issues"
  },
  "homepage": "https://gitlab.com/samuelbirk-ops/mac-cli#readme",
  "dependencies": {
    "@octokit/rest": "^17.2.0",
    "@types/shelljs": "^0.8.7",
    "@types/underscore": "^1.9.4",
    "chalk": "^4.0.0",
    "clear": "^0.1.0",
    "commander": "^5.0.0",
    "figlet": "^1.3.0",
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "shelljs": "^0.8.3",
    "underscore": "^1.10.2",
    "yaml": "^1.9.2",
    "yargs": "^15.3.1"
  },
  "devDependencies": {
    "@types/node": "^13.11.1",
    "@types/yaml": "^1.9.7",
    "@types/yargs": "^15.0.4",
    "nodemon": "^2.0.3",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  }
}
